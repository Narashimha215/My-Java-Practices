<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<bean id = "emp" class = "com.kn.entities.Employee">
		<property name="technologies">
			<list>
				<value>Java</value>
				<value>Python</value>
				<value>SQL</value>
			</list>
		</property>
		<property name="projects">
			<set>
				<value>Project-01</value>
				<value>Project-02</value>
				<value>Project-03</value>
			</set>
		</property>
		<property name="organization">
			<map>
				<entry key="org1" value="TCS"/>
				<entry key="org2" value="Infosys"/>
				<entry key="org3" value="Wipro"/>
				<entry key="org4" value="HCL"/>
			</map>
		</property>
	</bean>
</beans>