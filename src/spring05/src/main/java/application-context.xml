<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<bean id = "trainer1" class = "com.kn.entities.Trainer">
		<property name="id" value="1234"></property>
		<property name="name" value="Narashimha"></property>
		<property name="salary" value="1000000"></property>
	</bean>
	<bean id = "university" class = "com.kn.entities.University">
		<property name="id" value="222"></property>
		<property name="name" value="AITS"></property>
		<property name="trainer" value="trainer1"></property>
	</bean>
	
</beans>