<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<bean id = "mobile" class = "com.kn.entities.Mobile">
		<property name="brand" value="Vivo"></property>
		<property name="RAM" value="16GB"></property>
		<property name="cost" value="100000"></property>
	</bean>
	<bean id = "person" class = "com.kn.entities.Person" autowire="byType">
		<property name="name" value="Narashimha"></property>
		<property name="age" value="22"></property>
	</bean>
</beans>